{
  "name": "snipsonian",
  "version": "0.15.4",
  "description": "Small re-usable javascript code snippets",
  "main": "src/index.js",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-links && gh-pages -d public",
    "copyLatestToVersion": "node ./bin/copyLatestToVersion",
    "createPrevVersionsList": "node ./bin/createPrevVersionsList",
    "develop": "gatsby develop",
    "generateDoc": "node ./bin/generateDoc",
    "lintJs": "esw --ext .js,.jsx components config loaders pages prev_versions src utils *.js",
    "lintJs:watch": "npm run lintJs -- --watch",
    "setup": "npm install && npm run createPrevVersionsList",
    "serve-build": "gatsby serve-build",
    "start": "npm run develop",
    "test": "npm run lintJs && jest",
    "preversion": "npm test && npm run build",
    "version": "npm run copyLatestToVersion && git add -A prev_versions pages/doc",
    "postversion": "git push && git push --tags && npm run createPrevVersionsList",
    "verifyDocumentation": "node ./bin/verifyDocumentation"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chalk": "^1.1.3",
    "clear": "^0.0.1",
    "del": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "gatsby": "^0.12.36",
    "gh-pages": "^0.12.0",
    "git-user-name": "^1.2.0",
    "highlight.js": "^9.8.0",
    "inquirer": "^3.1.1",
    "jest": "^20.0.1",
    "js-beautify": "^1.6.14",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "15.5.4",
    "react-helmet": "^3.2.2",
    "react-highlight": "^0.10.0",
    "react-redux": "^5.0.3",
    "react-router": "2.8.1",
    "redux": "^3.6.0",
    "semver": "^5.3.0",
    "yargs": "^8.0.2"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.kunstmaan.be:9999/kunstmaan/snipsonian.git"
  },
  "keywords": [
    "kunstmaan",
    "javascript",
    "code",
    "snippet",
    "snippets",
    "ecmascript",
    "es6",
    "es2015"
  ],
  "author": "Kunstmaan",
  "license": "ISC"
}
