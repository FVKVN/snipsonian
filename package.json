{
  "name": "snipsonian",
  "version": "0.3.0",
  "description": "Small re-usable javascript code snippets",
  "main": "src/index.js",
  "scripts": {
    "createPrevVersionsList": "node ./bin/createPrevVersionsList.js",
    "setup": "yarn install && npm run createPrevVersionsList",
    "lintJs": "esw --ext .js,.jsx components config loaders pages prev_versions src utils *.js",
    "lintJs:watch": "npm run lintJs -- --watch",
    "develop": "gatsby develop",
    "test": "npm run lintJs && jest",
    "build": "gatsby build",
    "serve-build": "gatsby serve-build",
    "deploy": "gatsby build --prefix-links && gh-pages -d public",
    "preversion": "yarn test && yarn build",
    "version": "yarn run copyLatestToVersion && git add -A prev_versions",
    "postversion": "git push && git push --tags && yarn run createPrevVersionsList",
    "copyLatestToVersion": "node ./bin/copyLatestToVersion"
  },
  "dependencies": {
    "babel-loader": "^6.4.1",
    "gatsby": "^0.12.36",
    "highlight.js": "^9.8.0",
    "js-beautify": "^1.6.14",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "15.5.4",
    "react-helmet": "^3.2.2",
    "react-highlight": "^0.10.0",
    "react-redux": "^5.0.3",
    "react-router": "2.8.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "gh-pages": "^0.12.0",
    "jest": "^20.0.1"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.kunstmaan.be:9999/kunstmaan/snippet-js.git"
  },
  "keywords": [
    "kunstmaan",
    "javascript",
    "code",
    "snippet",
    "snippets",
    "ecmascript",
    "es6",
    "es2015"
  ],
  "author": "Kunstmaan",
  "license": "ISC"
}
