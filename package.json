{
  "name": "kunstmaan-snippet-js",
  "version": "0.1.0",
  "description": "Small re-usable javascript code snippets",
  "main": "src/index.js",
  "scripts": {
    "lint:js": "eslint src/ doc/",
    "unit": "karma start",
    "test": "npm run lint:js && npm run unit",
    "sass": "node-sass assets/scss/docApp.scss src/_doc/css/docApp.css",
    "build": "npm run test && npm run sass",
    "preserve": "npm run sass  # autoruns before the serve task",
    "serve": "browser-sync start --server './' --files 'src'",
    "watch:js": "watch 'npm run test' src/ --ignoreDirectoryPattern=/css/",
    "watch:scss": "watch 'npm run sass' assets/scss/",
    "dev": "npm-run-all --parallel serve watch:*"
  },
  "devDependencies": {
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "bootstrap": "4.0.0-alpha.5",
    "browser-sync": "2.17.5",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "jasmine": "2.5.2",
    "karma": "1.3.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2",
    "karma-systemjs": "0.15.0",
    "node-sass": "3.13.0",
    "npm-run-all": "3.1.1",
    "systemjs": "0.19.40",
    "systemjs-plugin-babel": "0.0.17",
    "vue": "2.0.5",
    "vue-router": "2.0.2",
    "watch": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.kunstmaan.be:9999/kunstmaan/snippet-js.git"
  },
  "keywords": [
    "kunstmaan",
    "javascript",
    "code",
    "snippet",
    "snippets",
    "ecmascript",
    "es6",
    "es2015"
  ],
  "author": "Kunstmaan",
  "license": "ISC"
}
